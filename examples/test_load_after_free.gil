import "stdlib_cap256.gil", "internals.gil", "unops_common.gil"; 

proc main() {
    y  := "i__malloc"("uint8");
    (* yn := y + 1 *)
    ys := "i__store"(y, {{ "uint8", 255i }});
    yl := "i__load"(y, "uint8");
